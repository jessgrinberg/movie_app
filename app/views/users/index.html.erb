<h1>All Users</h1>


<% @users.each do |user| %>
<ul>
<%= user.name %>

</ul>

<%end%>

<%if current_user%>
	You are logged in as <%= current_user.name if current_user %>

  <%= link_to 'LOG OUT!', logout_path, method: :delete %>

  <%else %>
	<%= link_to 'SIGN UP', users_new_path %>
	<%= link_to 'LOG IN!', login_path%>

<%end%>
